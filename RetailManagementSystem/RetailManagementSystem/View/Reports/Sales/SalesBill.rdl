      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="?">
            <Value>=Parameters!Parameter1.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select s.AddedOn,(select Name as 'Name' from customers where id = s.customerId) as Customer,CustomerOrderNo,TransportCharges,RunningBillNo,
s.Discount,
CASE
        WHEN s.PaymentMode = '0' THEN "Cash"
        ELSE "Credit"
    END as PaymentMode,
TotalAmount,
sd.Discount as ItemDiscount,
(select Price from priceDetails pd where pd.PriceId = sd.PriceId and pd.ProductId = sd.ProductId) as Price,
(select p.Name from Products p where p.Id = sd.ProductId ) as ProductName,
sd.Qty,
sd.SellingPrice
from sales s, SaleDetails sd
where s.BillId = sd.BillId
and s.billId = ?</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
