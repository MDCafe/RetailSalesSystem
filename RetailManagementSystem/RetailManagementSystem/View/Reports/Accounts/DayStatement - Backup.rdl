        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="CustomerPaymentsDataSet">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select c.name, sum(AmountPaid) AmonutPaid from PaymentDetails pd, Customers c 
where date(paymentDate) = "2019-06-23" and pd.customerId = c.Id
group by c.Id</CommandText>
