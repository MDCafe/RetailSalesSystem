  <DataSets>
    <DataSet Name="SalesDataSet">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select sum(TotalAmount) from (
select sum(AmountPaid) as TotalAmount from sales where date(AddedOn) = "2019-06-22" and (PaymentMode = 1 or PaymentMode = 2)
union all
select sum(TotalAmount) as TotalAmount from sales where date(AddedOn) = "2019-06-22" and PaymentMode = 0
) a;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="sum_TotalAmount_">
          <DataField>sum(TotalAmount)</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
