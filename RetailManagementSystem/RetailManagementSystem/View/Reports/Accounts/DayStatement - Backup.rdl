    <DataSet Name="ExpDataSet">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select cm.Description,sum(amount) Amount from ExpenseDetails exp, CodeMaster cm 
where 
date(addedOn) = "2019-06-22" and cm.Id = exp.ExpenseTypeId
group by exp.ExpenseTypeId</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
