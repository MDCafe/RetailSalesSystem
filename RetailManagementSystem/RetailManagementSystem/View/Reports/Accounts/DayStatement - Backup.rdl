          <DataField>Amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="CashPurchaseDataSet">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select c.Name,p.TotalBillAmount from Purchases p, companies c
where paymentMode = 0 and date(p.addedOn) = "2019-06-22"
and p.CompanyId = c.Id</CommandText>
      </Query>
      <Fields>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
