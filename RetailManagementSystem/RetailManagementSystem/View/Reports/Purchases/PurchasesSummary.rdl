  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select Billid,AddedOn,(select Name as 'Name' from Companies where id = p.companyId) as Supplier,InvoiceNo,
	   Discount,SpecialDiscount,Tax,TransportCharges,CoolieCharges,KCoolieCharges,LocalCoolieCharges,
	CASE
        WHEN p.isCancelled =1 THEN 'Cancelled'
        ELSE NULL
    END AS 'Cancelled',
RunningBillNo,
CASE
        WHEN p.PaymentMode = '0' THEN TotalBillAmount
        ELSE NULL
    END AS 'Cash Sales',
    CASE
        WHEN PaymentMode = '1'  THEN TotalBillAmount
        ELSE NULL
    END AS 'Credit Sales',
TotalBillAmount

from purchases p</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
