  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select p.Name ,sum(st.Quantity) 'Available Qty', c.name 'CategoryName', p.ReOrderPoint
from stocks st, Products p,Category c
where 
st.ProductId = p.id
and p.CategoryId = c.Id
group by c.id,st.ProductId,p.ReorderPoint
having Sum(st.Quantity) &lt;= p.ReorderPoint
order by c.name,p.Name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
