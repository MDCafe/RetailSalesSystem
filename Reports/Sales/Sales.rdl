  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select AddedOn,(select Name from customers where id = s.customerId) as Customer,
RunningBillNo,
CASE
        WHEN s.PaymentMode = '0' THEN TotalAmount
        ELSE NULL
    END AS 'Cash Sales',
    CASE
        WHEN PaymentMode = '1'  THEN TotalAmount
        ELSE NULL
    END AS 'Credit Sales',
TotalAmount

from sales s</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
